<title>Insert title here</title>
	<script type="text/javascript">
	
	// 변수 num을 선언하고 숫자 100을 저장
	var num = 100;
	//변수 str을 선언하고 자바스크립트를 저장
	var str = "자바스크립트";
	//각 변수에 저장된 데이터의 형(데이터종류)을 웹브라우저에 출력
	document.write(typeof(num) , "<br/>", typeof(str));
	document.write(typeof(str));
	
	</script>
