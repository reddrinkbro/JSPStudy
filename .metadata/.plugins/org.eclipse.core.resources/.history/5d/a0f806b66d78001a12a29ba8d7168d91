<title>Insert title here</title>

	<script type="text/javascript">
	
		//배열 생성 방식3.
	//	var 참조변수선언 = [값1,값2,값3];
		var d = [30,"홍길동",true];
//				  0    1     2 
	//  인덱스 위치에 접근하여 값을 얻어오는 방법
//		-> 참조변수명[인덱스위치번호] : 인덱스위치번호에 대응되는 값을 얻을수 있다.
		//document.write( d[0],"<br/>");
		//document.write( d[1],"<br/>");
		//document.write( d[2],"<br/>");
		
		document.write("<hr/>");
		//for문을 이용하여 배열메모리에 저장된 데이터를 하나씩 꺼내와 출력
		for (var i = 0; i < d.length; i++) {
			document.write(d[i],"<br/>"); //인덱스 0, 1, 2에 저장된 값을 얻어 출력함
		}
	</script>
