	<script type="text/javascript">
		//배열 객체의 메소드인 splice()와 slice()를 사용한예제
		//배열 객체 메소드 중 splice()메소드를 이용하여 배열에 지정한 인덱스의 데이터를 삭제한후
		//새로운 데이터를 삽입하였고, slice()메소드를 이용하여 배열의 일부 데이터만 잘라내서
		//새로운 배열 메모리에 저장후 가져온다
		var greenLine = ["사당", "교대", "방배", "강남"];
		var yellowLien = ["미금", "정자", "모란", "수서"];
		
		//순서1. var greenLine = ["사당","교대","방배","강남"];
		//순서2. var greenLien = ["사당","교대"," ","강남"];
		//순서3. var greenLine = ["사당","교대","서초","역삼","강남"];
		
		greenLine.splice(2, 1, "서초","역삼");
		
		document.write(greenLine,"<br/>");
	
		//배열 객체의 인덱스 1위치의 값 "교대"부터 인덱스 3위치 이전인 인덱스2위치에 있는 "서초"까지의
		//일부 데이터만 선택하여 새로운 배열로 만든 다음 배열 자체를 리턴함
		//var greenLine = ["사당", "고대", "서초", "역삼", "강남"];
		var a = greenLine.slice(1,3);
		document.write(a,"<br/>");
		
	</script>
