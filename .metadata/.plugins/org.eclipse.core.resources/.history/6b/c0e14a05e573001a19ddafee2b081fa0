<!DOCTYPE html>
<html>
<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
	<script type="text/javascript">
	//a라는 이름의 변수를 선언하고 10을 저장한다
	var a = 10;
	
	//b라는 이름의 변수를 선언하고 20을 저장한다
	var b = 20;
	
	//c라는 이름의 변수를 선언하고 10을 저장한다
	var c = 10;
	
	//f라는 이름의 변수를 선언하고 문자열 20을 저장한다
	var f = "20";
	
	//result라는 이름의 변수를 선언한다
	var result;
	
	//result 변수에 a의 값이 b보다 크냐? 라는 식의 결과 값을 저장
	result = a > b;
	document.write(result+ "<br>");
	
	//result 변수에 b의 값이 f의 값과 같냐? 라는 식의 결과 값을 저장
	//자바스크립트에서 문자열에 숫자만 있을 경우 숫자와 비교가 가능하다 
	result = b == f;
	document.write(result+ "<br>");
	
	//result 변수에 a의 값이 b과 다르냐? 라는 식의 결과 값을 저장
	result = a != b;
	document.write(result+ "<br>");
	
	//result 변수에 b와 f의 변수 타입이 같냐? 라는 식의 결과 값을 저장
	result = b === f;
	document.write(result+ "<br>");
	
	document.write(3^24);
	</script>
</head>
<body>

</body>
</html>