	<script type="text/javascript">
	
	var t1 = "학교종이";
	var t2 = "땡땡땡";
	var t3 = 8282;
	var t4 = "어서모이자";
	var result;
	//변수 result에 문자열 +문자열 + 슛자 + 문자열이므로
	//하나의 문자열로 결합된 데이터가 저장됨
	//참고 : 문자열과 숫자의 연산에서 숫자는 문자열로 변환된 다음 + 연산을 하게 됨.
	result = t1 + t2 + t3 + t4;
	document.write(result);
	
	</script>
